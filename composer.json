{
    "name": "mavimo/phpstan-psr-cache",
    "description": "PSR cache backend for phpstan",
    "type": "phpstan-extension",
    "require": {
        "phpstan/phpstan": "^0.12",
        "psr/cache": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Vito Moscaritolo",
            "email": "mavimo@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^0.12",
        "slevomat/coding-standard": "^6.0",
        "phpstan/phpstan-phpunit": "^0.12.6",
        "phpstan/extension-installer": "^1.0",
        "jangregor/phpstan-prophecy": "^0.6.2",
        "infection/infection": "^0.15.3"
    },
    "autoload": {
        "psr-4": {
            "Mavimo\\PHPStan\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mavimo\\Tests\\PHPStan\\Cache\\": "tests/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
